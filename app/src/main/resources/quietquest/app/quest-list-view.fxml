    <children>
        <ListView fx:id="questListView" layoutX="40.0" layoutY="125.0" prefHeight="200.0" prefWidth="200.0" />
        <Label layoutX="40.0" layoutY="57.0" text="Quests">
            <font>
                <Font size="37.0" />
            </font>
        </Label>
        <Button fx:id="deleteButton" layoutX="261.0" layoutY="125.0" mnemonicParsing="false" onAction="#onDeleteQuest" text="Delete" />
      <Button layoutX="260.0" layoutY="175.0" mnemonicParsing="false" text="Start" />
    </children>
</AnchorPane>
